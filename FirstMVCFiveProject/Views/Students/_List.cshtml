@model IEnumerable<FirstMVCFiveProject.Models.Student>
<table class="table table-striped table-hover rounded shadow-sm">
    <thead class="thead-dark">
        <tr>
            <th>Id</th>
            <th>Roll No</th>
            <th>Age</th>
            <th>Email</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.RollNo</td>
            <td>@item.Age</td>
            <td>@item.Email</td>
            <td class="text-end">
                <button class="btn btn-sm btn-info me-1 rounded js-details" data-id="@item.Id">Details</button>
                <button class="btn btn-sm btn-warning me-1 rounded js-edit" data-id="@item.Id">Edit</button>
                <button class="btn btn-sm btn-danger rounded js-delete" data-id="@item.Id">Delete</button>
            </td>
        </tr>
    }
    </tbody>
</table>
